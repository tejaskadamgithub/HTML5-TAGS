   <!--caption tag-->
   <h1>The caption element</h1>
    <table>
  <caption>Monthly savings</caption>
  <tr>
    <th>Month</th>
    <th>Savings</th>
  </tr>
  <tr>
    <td>January</td>
    <td>$100</td>
  </tr>
  <tr>
    <td>February</td>
    <td>$50</td>
  </tr>
  <style>
    table, th, td {
      border: 1px solid black;
    }
    </style>
</table>
